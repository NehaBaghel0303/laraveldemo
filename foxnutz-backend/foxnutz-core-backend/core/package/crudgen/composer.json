{
    "name": "devs-buddy/crudgen",
    "version": "0.1.0-Alpha",
    "description": "Generate full crud functionality in laravel project by using our easy to use GUI.",
    "keywords": [
        "devs-buddy",
        "crudgen"
    ],
    "homepage": "https://github.com/devs-buddy/crudgen",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Shoaib Khan",
            "email": "devsbuddy404@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0",
        "appzcoder/crud-generator": "^3.2"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "DevsBuddy\\Crudgen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevsBuddy\\Crudgen\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevsBuddy\\Crudgen\\CrudgenServiceProvider"
            ],
            "aliases": {
                "Crudgen": "DevsBuddy\\Crudgen\\CrudgenFacade"
            }
        }
    }
}
